URxvt.depth: 32
URxvt.background: [79]#000000

URxvt.scrollBar: false

!URxvt.font: xft:Monaco:size=12
!URxvt.font: xft:DejaVu Sans Mono for Powerline
!URxvt.font: xft:Dejavu\ Sans\ Mono:regular:size=13
!URxvt.font: xft:Liberation\ Mono:pixelsize=12

!URxvt*font: xft:Terminess\ Powerline:regular:size=13
!URxvt*font: xft:Monaco\ for\ Powerline:regular:size=13
!URxvt*font: xft:Bitstream\ Vera\ Sans\ Mono:regular:size=13

!URxvt*font:                 xft:Monaco for Powerline:regular:size=12:antialias=true
!URxvt*imFont:               xft:Monaco for Powerline:regular:size=12:antialias=true
!URxvt*boldFont:             xft:Monaco for Powerline:bold:size=12:antialias=true
!URxvt*italicFont:           xft:Monaco for Powerline:italic:size=12:antialias=true
!URxvt*boldItalicFont:       xft:Monaco for Powerline:bold:italic:size=12:antialias=true

URxvt*font:                 xft:Meslo LG L DZ for Powerline:regular:size=12:antialias=true
URxvt*imFont:               xft:Meslo LG L DZ for Powerline:regular:size=12:antialias=true
URxvt*boldFont:             xft:Meslo LG L DZ for Powerline:bold:size=12:antialias=true
URxvt*italicFont:           xft:Meslo LG L DZ for Powerline:italic:size=12:antialias=true
URxvt*boldItalicFont:       xft:Meslo LG L DZ for Powerline:bold:italic:size=12:antialias=true


!URxvt.font:                 xft:Monaco for Powerline:regular:size=12
!URxvt.imFont:               xft:Monaco for Powerline:regular:size=12
!URxvt.boldFont:             xft:Monaco for Powerline:bold:size=12
!URxvt.italicFont:           xft:Monaco for Powerline:italic:size=12
!URxvt.boldItalicFont:       xft:Monaco for Powerline:bold:italic:size=12

URxvt.urgentOnBell: true
URxvt.cursorBlink: true

URxvt.colorIT:      #87af5f
URxvt.colorBD:      #d7d7d7
URxvt.colorUL:      #87afd7

