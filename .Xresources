!font and locale
xterm*utf8: 1
xterm*locale: true
xterm*utf8Title: true
xterm*renderFont: true
xterm*preeditType: Root
xterm*xftAntialias: true
xterm*faceName: Bitstream Vera Sans Mono:pixelsize=15
xterm*faceNameDoublesize: Bitstream Vera Sans Mono:pixelsize=13

!tango color scheme
xterm*color0: #1e1e1e
